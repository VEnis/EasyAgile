{% extends "::base.html.twig" %}

{% block body %}
<h1>New Session</h1>

<form action="{{ path('poker_session_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit" class="btn btn-primary">Create new Session</button> or
        <a href="{{ path('poker_session') }}" class="btn">
            Cancel and back to the sessions list
        </a>
    </p>
</form>


{% endblock %}
